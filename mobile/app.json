{
  "expo": {
    "name": "AmunX",
    "slug": "amunx",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "scheme": "amunx",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amunx.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "AmunX needs access to your microphone to record audio episodes.",
        "NSCameraUsageDescription": "AmunX needs access to your camera for profile photos.",
        "NSPhotoLibraryUsageDescription": "AmunX needs access to your photo library.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:amunx.app"
      ]
    },
    "android": {
      "package": "com.amunx.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-av",
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.4"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f8c5fc3c-f67a-4ac9-a53b-2e93e491547f"
      }
    },
    "owner": "jonyboyko"
  }
}
